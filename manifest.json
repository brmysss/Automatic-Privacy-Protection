{
    "manifest_version": 3,
    "name": "自动隐私模式",
    "description": "自动隐私模式",
    "version": "0.1",
    "permissions": [
        "storage",
        "tabs",
        "history",
        "webNavigation",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
